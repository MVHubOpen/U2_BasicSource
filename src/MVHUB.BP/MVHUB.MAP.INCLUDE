    SENTENCE=@SENTENCE
*
* Universal Sentence Parser
*
    POS=0 ; QUOTE=@FALSE ; 
    LASTC="&&&&" ; SEN=""
    LOOP
        POS+=1 ; C=SENTENCE[POS,1]
    UNTIL C="" DO
        BEGIN CASE
            CASE C='"'
                QUOTE=NOT(QUOTE)
            CASE C=" " AND NOT(QUOTE)
                IF LAST.C#@AM THEN SEN:=@AM
            CASE 1
                SEN:=C
        END CASE
        LAST.C=C
    REPEAT
    SEN=TRIM(SEN,@AM)
*
    IF SEN<1>="RUN" THEN
        SEN=DELETE(SEN,1) ; SEN=DELETE(SEN,1)
    END
